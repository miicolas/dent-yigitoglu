---
import CardTY from "../components/CardTY.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout title="Rendez-vous">

    <section class="flex lg:flex-row flex-col">
        <div class="bg-gradient-to-r bg-blue-900 to-blue-500 lg:w-1/2 flex-none">
            <h3 class="text-3xl text-white p-10">Pour prendre rendez vous</h3>
            <div class="flex justify-center w-full pb-10">
                <CardTY />
            </div>
            
        </div>
        <div class="flex-1 w-full">
            <iframe  class="h-full w-full" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2624.680769027544!2d2.2864685000000002!3d48.864297500000006!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47e66ffb08a99a4d%3A0x8cc4bedfda742ce1!2s11%20Av.%20Raymond%20Poincar%C3%A9%2C%2075016%20Paris!5e0!3m2!1sfr!2sfr!4v1692742586398!5m2!1sfr!2sfr" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>

    </section>
    
</Layout>